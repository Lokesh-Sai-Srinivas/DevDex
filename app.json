{
  "expo": {
    "name": "DevDex",
    "slug": "DevDex",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "devdex",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true, 
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#121212"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.ayika.devdex",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#121212"
      },
      "androidNavigationBar": {
        "backgroundColor": "#121212",
        "barStyle": "light-content",
      },
      "backgroundColor": "#121212",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "b4b308a5-cc5d-470b-9f8e-1e738982bbcf"
      }
    }
  }
}